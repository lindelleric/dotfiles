"brightnessctl set +5%"
   XF86MonBrightnessUp

"brightnessctl set 5%-"
   XF86MonBrightnessDown

# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +2000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -2000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

"flameshot gui"
    m:0x0 + c:107
    Print

"arandr"
    m:0x0 + c:235
    XF86Display

